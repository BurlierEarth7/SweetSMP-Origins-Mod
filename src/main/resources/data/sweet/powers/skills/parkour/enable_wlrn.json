{
  "type": "origins:action_over_time",
  "entity_action": {
    "type": "origins:execute_command",
    "command": "execute as @s run tag @s add can_wallrun"
  },
  "interval": 1,
  "condition": {
    "type": "origins:and",
    "conditions": [
      {
        "type": "origins:resource",
        "resource": "sweet:skills/parkour/stamina_stamina",
        "comparison": ">=",
        "compare_to": 20
      },
      {
        "type": "origins:scoreboard",
        "objective": "toggle_wallrun",
        "comparison": "==",
        "compare_to": 0
      }
    ]
  },
  "hidden": true
}
